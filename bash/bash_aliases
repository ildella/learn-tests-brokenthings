alias l='ls -CFltrh'
alias la='ls -A'
alias gs='git status'
alias gd='git diff'
alias gdno='git diff --name-only'
alias gf='git fetch'
alias gb='git branch'
alias gr='git remote -v'
alias gg='git log --graph --oneline --all'
alias r='redis-cli'
alias folder="du -sk * | sort -n"
alias s='ddgr'
alias dll="doctl compute droplet list"
alias dl="doctl compute droplet list --format ID,Name,PublicIPv4,PrivateIPv4,Memory,VCPUs,Disk,Region,Image,Status"
